{
  "name": "ls",
  "version": "1.0.0",
  "description": "separation of concerns for front end and back end",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:api start:www",
    "start:api": "npm run start --prefix api",
    "start:www": "npm run start --prefix www",
    "build": "run-p build:api build:www",
    "build:api": "cd api && npm run build && cd ..",
    "build:www": "cd www && npm run build && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
